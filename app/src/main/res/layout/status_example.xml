<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/progresscircle"
        android:layout_width="115dp"
        android:layout_height="112dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progressbar_width="15dp"
        app:cpb_progress_max="100"
        app:cpb_progress="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.145"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.089" />

    <TextView
        android:id="@+id/progressbar_text"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/progresscircle"
        app:layout_constraintEnd_toEndOf="@+id/progresscircle"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="@+id/progresscircle"
        app:layout_constraintTop_toTopOf="@+id/progresscircle"
        app:layout_constraintVertical_bias="0.451" />

    <com.mikhaellopez.circularprogressbar.CircularProgressBar
        android:id="@+id/progresscircle2"
        android:layout_width="115dp"
        android:layout_height="112dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:cpb_background_progressbar_width="5dp"
        app:cpb_progressbar_width="15dp"
        app:cpb_progress_max="100"
        app:cpb_progress="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.144"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progresscircle"
        app:layout_constraintVertical_bias="0.123" />

    <TextView
        android:id="@+id/progressbar_text2"
        android:layout_width="72dp"
        android:layout_height="42dp"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/progresscircle2"
        app:layout_constraintEnd_toEndOf="@+id/progresscircle2"
        app:layout_constraintHorizontal_bias="0.675"
        app:layout_constraintStart_toStartOf="@+id/progresscircle2"
        app:layout_constraintTop_toTopOf="@+id/progresscircle2"
        app:layout_constraintVertical_bias="0.542" />

    <me.itangqi.waveloadingview.WaveLoadingView
        android:id="@+id/waveloading"
        android:layout_width="128dp"
        android:layout_height="128dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"

        app:layout_constraintBottom_toBottomOf="@+id/progresscircle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/progresscircle"
        app:layout_constraintTop_toTopOf="@+id/progresscircle"

        app:layout_constraintVertical_bias="0.0"
        app:wlv_borderColor="@color/colorLightPink"
        app:wlv_borderWidth="3dp"
        app:wlv_round_rectangle="true"

        app:wlv_shapeType="circle"
        app:wlv_titleBottomSize="24dp"
        app:wlv_titleBottomStrokeColor="@color/colorWhite"
        app:wlv_titleBottomColor="@color/colorLightPink"
        app:wlv_titleBottomStrokeWidth="3dp"

        app:wlv_titleCenterColor="@color/colorLightOrange"
        app:wlv_titleCenterSize="24dp"
        app:wlv_titleCenterStrokeColor="@color/colorWhite"
        app:wlv_titleCenterStrokeWidth="3dp"

        app:wlv_titleTopColor="@color/colorLightGreen"
        app:wlv_titleTopSize="24dp"
        app:wlv_titleTopStrokeColor="@color/colorWhite"
        app:wlv_titleTopStrokeWidth="3dp"
        app:wlv_triangle_direction="north"
        app:wlv_waveAmplitude="50" />

    <me.itangqi.waveloadingview.WaveLoadingView
        android:id="@+id/waveloading2"
        android:layout_width="128dp"
        android:layout_height="128dp"

        android:layout_marginStart="8dp"

        android:layout_marginEnd="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/progresscircle2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/progresscircle2"

        app:layout_constraintTop_toTopOf="@+id/progresscircle2"
        app:layout_constraintVertical_bias="0.0"
        app:wlv_borderWidth="3dp"
        app:wlv_round_rectangle="true"
        app:wlv_shapeType="circle"
        app:wlv_titleBottomColor="@color/colorLightPink"
        app:wlv_titleBottomSize="24dp"
        app:wlv_titleBottomStrokeColor="@color/colorWhite"
        app:wlv_titleBottomStrokeWidth="3dp"

        app:wlv_titleCenterColor="@color/colorLightOrange"
        app:wlv_titleCenterSize="24dp"
        app:wlv_titleCenterStrokeColor="@color/colorWhite"
        app:wlv_titleCenterStrokeWidth="3dp"

        app:wlv_titleTopColor="@color/colorLightGreen"
        app:wlv_titleTopSize="24dp"
        app:wlv_titleTopStrokeColor="@color/colorWhite"
        app:wlv_titleTopStrokeWidth="3dp"
        app:wlv_triangle_direction="north"
        app:wlv_waveAmplitude="50"/>


    <ProgressBar
        android:id="@+id/progressBar1"
        style="@android:style/Widget.ProgressBar.Horizontal"
        android:layout_width="249dp"
        android:layout_height="26dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:max="100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/waveloading"
        app:layout_constraintVertical_bias="0.478" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:textSize="20dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/progressBar1"
        app:layout_constraintVertical_bias="0.193" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="TextView"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.337"
        app:layout_constraintStart_toEndOf="@+id/textView9"
        app:layout_constraintTop_toBottomOf="@+id/progressBar1"
        app:layout_constraintVertical_bias="0.191" />
</android.support.constraint.ConstraintLayout>